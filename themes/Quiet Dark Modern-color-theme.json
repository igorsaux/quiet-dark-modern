    "semanticHighlighting": true,
    "tokenColors": [
        {
            "settings": {
                "foreground": "#C7A4DB"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded",
                "string meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#C7A4DB"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#506686"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#506686"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation",
                "comment.block.documentation punctuation.definition.comment "
            ],
            "settings": {
                "foreground": "#8eca76"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#f25b5b"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#b07d7d"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {
                "foreground": "#c7a4db"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#c7a4db"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#d87c77"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#a4dba4"
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {
                "foreground": "#b07d7d"
            }
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#8eca76"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "HTML: Doctype Declaration",
            "scope": [
                "meta.tag.sgml.doctype",
                "meta.tag.sgml.doctype string",
                "meta.tag.sgml.doctype entity.name.tag",
                "meta.tag.sgml punctuation.definition.tag.html"
            ],
            "settings": {
                "foreground": "#AAAAAA"
            }
        },
        {
            "name": "HTML: Tags",
            "scope": [
                "meta.tag",
                "punctuation.definition.tag.html",
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#91B3E0"
            }
        },
        {
            "name": "HTML: Tag Names",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "HTML: Attribute Names",
            "scope": [
                "meta.tag entity.other.attribute-name",
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#8190A0"
            }
        },
        {
            "name": "HTML: Entities",
            "scope": [
                "constant.character.entity",
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "CSS: Selectors",
            "scope": [
                "meta.selector",
                "meta.selector entity",
                "meta.selector entity punctuation",
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#c7a4db"
            }
        },
        {
            "name": "CSS: Property Names",
            "scope": [
                "meta.property-name",
                "support.type.property-name"
            ],
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "CSS: Property Values",
            "scope": [
                "meta.property-value",
                "meta.property-value constant.other",
                "support.constant.property-value"
            ],
            "settings": {
                "foreground": "#8eca76"
            }
        },
        {
            "name": "CSS: Important Keyword",
            "scope": "keyword.other.important",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#bababa"
            }
        },
        {
            "name": "Markup: Deletion",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#bababa"
            }
        },
        {
            "name": "Markup: Emphasis",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markup: Error",
            "scope": "markup.error",
            "settings": {
                "foreground": "#b07d7d"
            }
        },
        {
            "name": "Markup: Insertion",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#bababa"
            }
        },
        {
            "name": "Markup: Link",
            "scope": "meta.link",
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "Markup: Output",
            "scope": [
                "markup.output",
                "markup.raw"
            ],
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Prompt",
            "scope": "markup.prompt",
            "settings": {
                "foreground": "#777777"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#d87c77"
            }
        },
        {
            "name": "Markup: Strong",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Traceback",
            "scope": "markup.traceback",
            "settings": {
                "foreground": "#b07d7d"
            }
        },
        {
            "name": "Markup: Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#c7a4db"
            }
        },
        {
            "name": "Markup Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "Markup Styling",
            "scope": [
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#8eca76"
            }
        },
        {
            "name": "Markup Inline",
            "scope": "markup.inline.raw",
            "settings": {
                "fontStyle": "",
                "foreground": "#e2934d"
            }
        },
        {
            "name": "Extra: Diff Range",
            "scope": [
                "meta.diff.range",
                "meta.diff.index",
                "meta.separator"
            ],
            "settings": {
                "foreground": "#434343"
            }
        },
        {
            "name": "Extra: Diff From",
            "scope": [
                "meta.diff.header.from-file",
                "punctuation.definition.from-file.diff"
            ],
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "Extra: Diff To",
            "scope": [
                "meta.diff.header.to-file",
                "punctuation.definition.to-file.diff"
            ],
            "settings": {
                "foreground": "#98ace9"
            }
        },
        {
            "name": "diff: deleted",
            "scope": "markup.deleted.diff",
            "settings": {
                "foreground": "#e86950"
            }
        },
        {
            "name": "diff: changed",
            "scope": "markup.changed.diff",
            "settings": {
                "foreground": "#e2934d"
            }
        },
        {
            "name": "diff: inserted",
            "scope": "markup.inserted.diff",
            "settings": {
                "foreground": "#8eca76"
            }
        },
        {
            "name": "JSX: Tags",
            "scope": [
                "punctuation.definition.tag.js",
                "punctuation.definition.tag.begin.js",
                "punctuation.definition.tag.end.js"
            ],
            "settings": {
                "foreground": "#91B3E0"
            }
        },
        {
            "name": "JSX: InnerText",
            "scope": "meta.jsx.children.js",
            "settings": {
                "foreground": "#bbbbbb"
            }
        }
    ]
}